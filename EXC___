-- Scripts:

local function SPXNUS_fake_script() -- TextLabel.Script (READ ME) 
	local script = Instance.new('LocalScript', TextLabel)

	--place this script INSIDE the gui instance you want to move/resize

	local TweenService = game:GetService("TweenService") --gets tweenService
	local tweenInstance = script.Parent --the gui you want to move/resize

	local tweenInfo = TweenInfo.new(1.2,Enum.EasingStyle.Quart,Enum.EasingDirection.Out,0) -- creates the tweens info (if you want it to repeat, change the 0 to how many times it should repeat.)

	local tween = TweenService:Create(tweenInstance, tweenInfo, {Position = UDim2.new({0.364, 0},{0, 0})}) --creates the tween

	tween:Play() --plays the tween

	--thanks for using My Tween Tool, leave a comment under this plugin if you have any questions or wanna report a bug. :)

end
coroutine.wrap(SPXNUS_fake_script)()
local function NMPE_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local frame = script.Parent

	frame.Active = true
	frame.Visible = true
	frame.Draggable = true
	frame.Selectable = true

end
coroutine.wrap(NMPE_fake_script)()
local function SOKUSQF_fake_script() -- TextLabel_4.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_4)

	script.Parent:TweenPosition(UDim2.new(0.191, 0,-0.004, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear)
end
coroutine.wrap(SOKUSQF_fake_script)()


TextBox.FocusLost:Connect(function()
	loadstring(TextBox.Text)()
end)

TextBox.FocusLost:Connect(function()
	wait()
	TextBox.PlaceholderText = "Yukio INT."
	TextBox.Text = ""
end)

TextBox.Focused:Connect(function()
	TextBox.PlaceholderText = "Waiting for your dumb ass fucking script."
end)

function StoreScript(Name,Callback)
	writefile(Name..".SC",Callback)
	local TextButton = Instance.new("TextButton")
	TextButton.Parent = Framea
	TextButton.BackgroundColor3 = GetColor()
	TextButton.BackgroundTransparency = 0.800
	TextButton.BorderColor3 = GetColor()
	TextButton.BorderSizePixel = 0
	TextButton.Size = UDim2.new(0, 120, 0, 14)
	TextButton.Font = Enum.Font.SourceSans
	TextButton.TextColor3 = Color3.fromRGB(255,255,255)
	TextButton.TextSize = 14.000
	TextButton.Text = Name
	TextButton.MouseButton1Down:Connect(function()
		loadstring(readfile(Name..".SC"))()
	end)

end



local ScriptToBeStored = [[loadstring(game:HttpGet("https://pastebin.com/raw/YMxgqJDA"))()]] -- Can be Obsfucated, Doesnt Matter.
StoreScript("Kyro",ScriptToBeStored)
