getgenv().InternalColor = ("Pink") 

function GetColor()
	if getgenv().InternalColor == ("Red") then 
		return Color3.fromRGB(216, 0, 4)
	end
    if getgenv().InternalColor == ("Pink") then 
		return Color3.fromRGB(255, 0, 144)
	end
    if getgenv().InternalColor == ("Blue") then 
		return Color3.fromRGB(0, 0, 255)
	end
    if getgenv().InternalColor == ("Yellow") then 
		return Color3.fromRGB(221, 232, 3)
	end
    if getgenv().InternalColor == ("Black") then 
		return Color3.fromRGB(0, 0, 0)
	end
end

